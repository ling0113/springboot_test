<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包-->
  <classPathEntry
    location="C:\Users\Administrator\.m2\repository\mysql\mysql-connector-java\5.1.46\mysql-connector-java-5.1.46.jar"/>
  <!-- MyBatis3Simple：不生成 Example相关类及方法 defaultModelType="flat" -->
  <context id="MysqlContext" targetRuntime="MyBatis3Simple">
    <!--<context id="DB2Tables"  targetRuntime="MyBatis3">-->

    <!--配置toString方法-->
    <!--<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />-->
    <!-- 扩展entity的set方法-->
    <!--<plugin rangeType="mybatis.generator.plugins.ExtendEntitySetter" />-->
    <!-- 配置pojo的序列化 -->
    <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
    <!-- 自动为entity生成swagger2文档-->
    <plugin type="mybatis.generator.plugins.GeneratorSwagger2Doc"/>
    <!-- 整合lombok-->
    <!--<plugin type="com.chrm.mybatis.generator.plugins.LombokPlugin" >
        <property name="hasLombok" value="true"/>
    </plugin>-->
    <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
      <property name="mappers" value="tk.mybatis.mapper.common.Mapper"/>
      <property name="caseSensitive" value="true"/>
      <property name="forceAnnotation" value="true"/>
      <property name="beginningDelimiter" value="`"/>
      <property name="endingDelimiter" value="`"/>
    </plugin>
    <commentGenerator>
      <property name="suppressDate" value="false"/>
      <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
      <property name="suppressAllComments" value="true"/>
    </commentGenerator>
    <!--数据库链接URL，用户名、密码 -->
    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
      connectionURL="jdbc:mysql://ling07.top:3306/test"
      userId="root"
      password="lgr123456">
    </jdbcConnection>
    <!--<javaTypeResolver>-->
    <!--<property name="forceBigDecimals" value="false"/>-->
    <!--</javaTypeResolver>-->
    <!--生成模型的包名和位置-->
    <javaModelGenerator targetPackage="com.springboot_test.entity"
      targetProject="E:\ling\demo08\springboot_test\src\main\java">
      <property name="enableSubPackages" value="true"/>
      <property name="trimStrings" value="true"/>
    </javaModelGenerator>
    <!--生成映射文件的包名和位置-->
    <sqlMapGenerator targetPackage="mybatis-mappers"
      targetProject="E:\ling\demo08\springboot_test\src\main\resources">
      <property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>
    <!-- 生成DAO的包名和位置-->
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.springboot_test.dao2"
      targetProject="E:\ling\demo08\springboot_test\src\main\java">
      <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>
    <!--要生成的表 tableName是数据库中的表名或视图名 domainObjectName是实体类名-->

    <!--<table tableName="tbl_yzb_payment_way" domainObjectName="PaymentWay" mapperName="PaymentWayDao"  enableUpdateByExample="false" enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">-->
    <!--</table>-->
    <table tableName="aa" domainObjectName="aa" mapperName="aaDao" enableUpdateByExample="false"
      enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>
    <!--<table tableName="tbl_yzb_bank_item" domainObjectName="BankItem" mapperName="BankItemDao"  enableUpdateByExample="false" enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
    </table>-->
  </context>
</generatorConfiguration>